extends layout

block content
  h1= title
  p Welcome to #{title}
  table 
    thead 
      tr 
        th Title 
        th Author 
        th Genre 
        th Year 
    tbody
      each book in books  
        tr 
          td #{book.dataValues.title}
          td #{book.dataValues.author}
          td #{book.dataValues.genre}
          td #{book.dataValues.year}